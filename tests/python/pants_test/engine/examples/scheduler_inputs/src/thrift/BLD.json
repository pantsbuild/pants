{
  "type_alias": "jar",
  "abstract": true,
  "org": "org.apache.thrift",
  "name": "libthrift"
}

{
  "type_alias": "jar",
  "org": "org.slf4j",
  "name": "slf4j-api",
  "rev": "1.6.1"
}

{
  "type_alias": "target",
  "abstract": true,
  "name": "java_thrift_base",
  "configurations": [
    {
      "type_alias": "apache_thrift_java_configuration",
      "name": "apache_java",
      "rev": "0.10.0",
      "strict": true,
      "dependencies": [
        {
          "type_alias": "jar",
          "extends": ":libthrift",
          "rev": "0.10.0"
        },
        {
          "type_alias": "jar",
          "org": "commons-lang",
          "name": "commons-lang",
          "rev": "2.5"
        },
        ":slf4j-api"
      ]
    }
  ]
}

{
  "type_alias": "scrooge_scala_configuration",
  "name": "scrooge_scala_config",
  "rev": "18.12.0",
  "strict": true,
  "dependencies": [
    {
      "type_alias": "jar",
      "extends": ":libthrift",
      "rev": "0.10.0"
    },
    {
      "type_alias": "jar",
      "org": "com.twitter",
      "name": "scrooge-core_2.11",
      "rev": "18.12.0"
    },
    {
      "type_alias": "jar",
      "org": "com.twitter",
      "name": "finagle-thrift_2.11",
      "rev": "18.12.0"
    },
    "3rdparty/jvm:scala-library"
  ]
}

{
  "type_alias": "target",
  "abstract": true,
  "name": "scala_thrift_base",
  "configurations": [
    ":scrooge_scala_config"
  ]
}

{
  "type_alias": "target",
  "abstract": true,
  "name": "all_jvm_thrift_base",
  "merges": [":java_thrift_base", ":scala_thrift_base"],
  "configurations": [
    {
      "type_alias": "scrooge_java_configuration",
      "name": "scrooge_java_config",
      "rev": "18.12.0",
      "strict": true,
      "dependencies": [
        ":slf4j-api",
        {
          "type_alias": "jar",
          "extends": ":libthrift",
          "rev": "0.10.0"
        }
      ]
    },
    {
      "type_alias": "apache_thrift_java_configuration",
      "name": "failing",
      "rev": "fail",
      "strict": true
    }
  ]
}
